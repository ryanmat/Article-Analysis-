<h1><%= current_user.name %></h1>
<p><%= link_to "Submit Article", new_article_path %></p>

<% current_user.articles.each do |article| %>
<div class="single-article">
	<h2><%= article.title %></h2>
	<h4><%= article.author %></h4>
	<%= link_to "View Article", article_path(article) %>
</div>
<% end %>
